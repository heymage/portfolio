{
  "env": {
    "es2021": true
  },
  "extends": [
    "next/core-web-vitals",
    "prettier"
  ],
  "rules": {
    "no-duplicate-imports": "error",
    "camelcase": "error",
    "eqeqeq": [
      "error",
      "smart"
    ],
    "yoda": [
      "error",
      "never",
      {
        "onlyEquality": true
      }
    ]
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  }
}
